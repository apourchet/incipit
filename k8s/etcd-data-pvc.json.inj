{
    "apiVersion": "v1",
    "kind": "PersistentVolumeClaim",
    "metadata": {
        "name": "etcd-data",
        "namespace": "incipit"
    },
    "spec": {
        "accessModes": ["ReadWriteOnce"],
        "resources": {
            "requests": {
                "storage": "1Gi"
            }
        },
        "volumeName": "etcd-data"
    }
}
