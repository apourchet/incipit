{
    "kind": "Service", 
    "spec": {
        "selector": {
            "k8s-app": "main-ingress-lb"
        }, 
        "type": "LoadBalancer", 
        "ports": [
            {
                "protocol": "TCP", 
                "targetPort": 80, 
                "port": 80, 
                "nodePort": {{.INGRESS_HTTP_PORT}},
                "name": "http"
            }, 
            {
                "protocol": "TCP", 
                "targetPort": 443, 
                "port": 443, 
                "nodePort": {{.INGRESS_HTTPS_PORT}}, 
                "name": "https"
            }
        ], 
        "sessionAffinity": "None"
    }, 
    "apiVersion": "v1", 
    "metadata": {
        "namespace": "{{.NAMESPACE}}", 
        "name": "main-ingress"
    }
}
