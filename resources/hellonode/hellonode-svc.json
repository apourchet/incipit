    "apiVersion": "v1", 
    "kind": "Service", 
    "metadata": {
        "namespace": "{{.NAMESPACE}}", 
        "name": "hellonode"
    },
    "spec": {
        "type": "ClusterIP", 
        "ports": [
            {
                "protocol": "TCP", 
                "targetPort": 8081, 
                "name": "http", 
                "port": 80
            }
        ], 
        "selector": {
            "k8s-app": "hellogo"
        }
    }
}
