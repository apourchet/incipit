{
    "apiVersion": "v1",
    "kind": "PersistentVolumeClaim",
    "metadata": {
        "name": "tmp",
        "namespace": "{{.NAMESPACE}}"
    },
    "spec": {
        "accessModes": ["ReadWriteOnce"],
        "resources": {
            "requests": {
                "storage": "1Gi"
            }
        },
        "volumeName": "tmp"
    }
}
