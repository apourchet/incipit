{
    "apiVersion": "v1", 
    "kind": "Service", 
    "metadata": {
        "namespace": "{{$.NAMESPACE}}", 
        "name": "auth"
    },
    "spec": {
        "type": "ClusterIP", 
        "ports": [
            {
                "protocol": "TCP", 
                "targetPort": 8080,
                "name": "http", 
                "port": 80
            }
        ], 
        "selector": {
            "auth": "true"
        }
    }
}
