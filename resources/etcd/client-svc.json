{
    "apiVersion": "v1", 
    "kind": "Service", 
    "metadata": {
        "namespace": "{{.NAMESPACE}}", 
        "name": "etcd-client"
    },
    "spec": {
        "type": "ClusterIP", 
        "ports": [
            {
                "name": "http", 
                "protocol": "TCP", 
                "port": 2379,
                "targetPort": 2379 
            }
        ], 
        "selector": {
            "etcd": "true"
        }
    }
}
